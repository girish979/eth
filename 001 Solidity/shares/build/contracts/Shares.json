  "source": "pragma solidity ^0.4.8;\r\ncontract Shares {\r\n  mapping (address => uint) shares;\r\n \r\n  function updateShares(uint share) public {\r\n    shares[msg.sender] = getShares(msg.sender) + share;\r\n  }\r\n \r\n  function getShares(address addr) public view returns(uint) {\r\n    return shares[addr];\r\n  }\r\n}",
